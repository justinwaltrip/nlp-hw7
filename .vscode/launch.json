    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Current File",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "justMyCode": true,
            "args": [
                "--experiment",
                "test",
                "--small_subset",
                "--num_epochs",
                "1",
                "--lr",
                "1e-3",
                "--batch_size",
                "4",
                "--device",
                "cpu",
                "--model",
                "facebook/rag-sequence-nq",
                "--include_gold_passage"
            ]
        }
    ]
}